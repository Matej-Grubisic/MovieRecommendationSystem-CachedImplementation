<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="75.0" prefWidth="350.0" styleClass="anchorpane" stylesheets="@styles.css" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="dk.easv.presentation.controller.FriendsController">
   <children>
      <Label fx:id="friendName" layoutX="14.0" layoutY="18.0" prefHeight="40.0" prefWidth="200.0" text="Username" textFill="WHITE">
         <font>
            <Font size="24.0" />
         </font>
      </Label>
      <Button fx:id="addbutton" layoutX="230.0" layoutY="18.0" mnemonicParsing="false" onAction="#addFriend" prefHeight="40.0" prefWidth="106.0" styleClass="buttonFriend" stylesheets="@styles.css" text="Add friend" textFill="WHITE">
         <font>
            <Font size="14.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
